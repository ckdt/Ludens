<!-- Sitemap -->
<!--Footer-->
<div class="row footer">
	<div class="col-lg-2 col-sm-2 col-xs-12">
	  <span class="title">sitemap</span>
    <ul class="footer-list">
			{% for item in menu_main.get_items %}
				<li class="{{item.classes | join(' ')}}">
					<a class="nav-link" href="{{item.get_link}}">{{item.title}}</a>
				</li>
			{% endfor %}
		</ul>
	</div>

	<!-- Services -->
	<div class="col-lg-2 col-sm-2 col-xs-12">
	  <span class="title">onze diensten</span>
	  <ul class="footer-list">
			{% for item in menu_footer.get_items %}
				<li class="nav-item {{item.classes | join(' ')}}">
					<a class="nav-link" href="{{item.get_link}}">{{item.title}}</a>
				</li>
			{% endfor %}
		</ul>
	</div>

	<!-- Social -->
	<div class="col-lg-2 col-sm-2 col-xs-12">
	  <span class="title">volg ons</span>
	  <ul class="footer-list">
		{% for item in menu_social.get_items %}
			<li class="nav-item {{item.classes | join(' ')}}">
				<a class="nav-link" href="{{item.get_link}}" target="_blank">{{item.title}}</a>
			</li>
		{% endfor %}
		</ul>
	</div>

	<!--contact gegevens -->
	<div class="col-lg-2 col-sm-2 col-xs-12">
	  <span class="title">contact</span>
	  <ul class="footer-list">
			<li>Koetshuis Den Dolder</li>
	    <li>Landgoed Ewijkshoeve</li>
	    <li>Soestdijkerweg 12a</li>
	    <li>3734 MH Den Dolder</li>
	  </ul>
	</div>

	<div class="col-lg-2 col-sm-2 col-xs-12">
	  <span class="footer-title opacity">menu</span>
	  <ul class="footer-list">
	    <li>info@ludenstraining.nl</li>
	    <li>035-6668456</li>
	  </ul>
	</div>
</div>


